<h1> index </h1>

<% @posts.each do |post| %>

    <%= post.id %><br>
    <%= post.title %><br>
    <%= post.content %><br>

    <%= link_to "show", post_path(post.id) %>
    <%= link_to "edit", edit_post_path(post.id) %>
    <%= link_to "destroy", post_path(post.id), method: :delete %><br>

<% end %>